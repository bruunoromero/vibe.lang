module Vibe.Pair exposing
    ( Pair(..)
    , first
    , second
    )

{-| Represents an ordered pair of values.

    Example:
        point : Pair Int Int
        point = Pair 3 4
-}
type Pair a b
    = Pair a b

first : Pair a b -> a
first (Pair a _) = a

second : Pair a b -> b
second (Pair _ b) = b
